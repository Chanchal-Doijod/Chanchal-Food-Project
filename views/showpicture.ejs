<html>
    <center>
        <form action="/food/edit_picture" method="post" enctype="multipart/form-data">
            <input type="hidden" name="foodid" value="<%=data.foodid %>"><br><br>
            <h2><%=data.foodname %></h2><br>
            <img src="/images/<%=data.foodpicture%>" width="200" height="200"><br><br>
            <B>Select New Picture</B><br><br>
            <input type="hidden" name="oldfoodpicture" value="<%=data.foodpicture%>"/><br><br>
            <input type="file" required name="foodpicture" accept="image/*"/><br><br>
            <input type="submit">
        </form>
    </center>
</html>